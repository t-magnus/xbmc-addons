<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="Animation_CommonFade">
		<animation effect="fade" start="0" end="100" time="200">WindowOpen</animation>
		<animation effect="fade" start="100" end="0" time="200">WindowClose</animation>
		<animation effect="fade" start="0" end="100" time="200">Visible</animation>
		<animation effect="fade" start="100" end="0" time="200">Hidden</animation>
	</include>
	
	<include name="Slide_NowPlaying">
		<animation effect="Slide" start="0,0" end="175,0" time="200" tween="quadratic" condition="Player.HasVideo">Conditional</animation>
	</include>
	
    <include name="dialogOpen">
        <animation type="WindowOpen" reversible="false">
            <effect type="fade" start="0" end="100" time="350" tween="Sine" easing="Out" />
			<effect type="zoom" start="80" end="100" center="640,360" easing="out" tween="back" time="300" />
        </animation>
		<animation type="WindowClose" reversible="false">
            <effect type="fade" start="100" end="0" time="350" tween="Sine" easing="Out" />
			<effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="back" time="300" />
        </animation>
        <animation type="Visible" reversible="false">
            <effect type="fade" start="0" end="100" time="350" tween="Sine" easing="Out" />
			<effect type="zoom" start="80" end="100" center="640,360" easing="out" tween="back" time="300" />
        </animation>
        <animation type="Hidden" reversible="false">
            <effect type="fade" start="100" end="0" time="350" tween="Sine" easing="Out" />
			<effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="back" time="300" />
        </animation>
    </include>
	
	<include name="WindowOpen">
        <animation type="WindowOpen" reversible="false">
            <effect type="fade" start="0" end="100" time="350" tween="Sine" easing="Out" />
			<effect type="zoom" start="120" end="100" center="640,360" easing="out" tween="Sine" time="350" />
        </animation>
		<animation type="WindowClose" reversible="false">
            <effect type="fade" start="100" end="0" time="350" tween="Sine" easing="Out" />
			<effect type="zoom" start="100" end="120" center="640,360" easing="in" tween="Sine" time="350" />
        </animation>
        <animation type="Visible" reversible="false">
            <effect type="fade" start="0" end="100" time="350" tween="Sine" easing="Out" />
			<effect type="zoom" start="80" end="100" center="640,360" easing="out" tween="Sine" time="350" />
        </animation>
        <animation type="Hidden" reversible="false">
            <effect type="fade" start="100" end="0" time="350" tween="Sine" easing="Out" />
			<effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="Sine" time="350" />
        </animation>
    </include>
	
	<include name="Window_Transition">	

		<animation type="WindowOpen" reversible="false">			
			<effect type="fade" start="0" end="100" time="400" />
			<effect type="rotatey" start="90" end="0" center="640,0" time="300" tween="quadratic" easing="out" />
			<effect type="zoom" start="40" end="100" center="640,360" delay="200" time="200" tween="quadratic" easing="out" />			
		</animation>
		
		<animation type="WindowClose">
			<effect type="fade" start="100" end="0" time="300" />
		</animation>
		
	</include>

<!--Video Library Gallery-->

    <include name="focus_Poster">
        <animation effect="zoom" delay="100" start="100" end="135" tween="quadratic" center="auto" time="200" reversible="true">Focus</animation>
        <animation effect="zoom" start="135" end="100" tween="quadratic" center="auto" time="200">Unfocus</animation>
    </include>
	<include name="focus_Panel">
        <animation effect="zoom" delay="100" start="100" end="135" tween="quadratic" center="auto" time="200" reversible="true">Focus</animation>
        <animation effect="zoom" start="135" end="100" center="auto" time="100">Unfocus</animation>
    </include>
	<include name="focus_Wide">
        <animation effect="zoom" delay="250" start="100" end="130" tween="back" center="auto" time="450" reversible="true">Focus</animation>
        <animation effect="zoom" start="130" end="100" center="auto" time="150">Unfocus</animation>
    </include>
	<include name="focus_Wide2">
		<animation type="Focus" reversible="true">
			<effect type="fade" delay="300" start="0" end="100" tween="quadratic" time="450" />
			<effect type="zoom" delay="300" start="100" end="130" tween="quadratic" center="auto" time="450" />
		</animation>
        <animation effect="zoom" start="130" end="100" center="auto" time="150">Unfocus</animation>
    </include>
	<include name="focus_Wide3">
		<animation type="Focus" reversible="true">
			<effect type="fade" delay="300" start="0" end="100" tween="quadratic" time="250" />
			<effect type="zoom" delay="300" start="100" end="150" tween="quadratic" center="auto" time="450" />
			<effect type="slide" delay="300" start="0,0" end="0,35" tween="back" time="450" />
		</animation>
        <animation effect="fade" start="100" end="0" time="50">Unfocus</animation>
    </include>
	
	<include name="focus_Thumb_Overylay">
        <animation effect="slide" delay="0" start="0" end="23,12" tween="quadratic" time="200">Focus</animation>
        <animation effect="slide" start="23,12" end="0" time="100">Unfocus</animation>
    </include>
	<include name="focus_Poster_Overlay">
        <animation effect="slide" delay="0" start="0" end="15,22" time="200">Focus</animation>
        <animation effect="slide" start="15,22" end="0" time="100">Unfocus</animation>
    </include>
	
<!--SubMenu Animations-->

	<include name="Fade_focus_SubMenu">
        <animation type="Focus" reversible="false">
			<effect type="fade" start="0" end="100" time="150" />
		</animation>
        <animation type="Unfocus" reversible="false">
			<effect type="fade" start="100" end="0" time="0" />
		</animation>
    </include>
	<include name="Zoom_focus_SubMenu">
        <animation type="Focus" reversible="false">
			<effect type="zoom" start="100" end="115" center="0,60" tween="quadratic" time="zoomtime_SubMenu" />
		</animation>
        <animation type="Unfocus" reversible="false">
			<effect type="zoom" start="115" end="100" center="0,60" tween="quadratic" time="zoomtime_SubMenu" />
		</animation>
    </include>
	<include name="Zoom_focus_SubMenuBG">
        <animation type="Focus" reversible="false">
			<effect type="zoom" start="100" end="120,125" center="25,60" tween="quadratic" time="zoomtime_SubMenu" />
		</animation>
        <animation type="Unfocus" reversible="false">
			<effect type="zoom" start="120,125" end="100" center="25,60" tween="quadratic" time="zoomtime_SubMenu" />
		</animation>
    </include>
	<include name="focus_SubMenuBG">
        <animation type="Focus" reversible="false">
			<effect type="zoom" start="100" end="102,104" center="auto" tween="quadratic" time="0" />
		</animation>
        <animation type="Unfocus" reversible="false">
			<effect type="zoom" start="110" end="100" center="auto" tween="quadratic" time="0" />
			<effect type="fade" start="100" end="0" time="0" />
		</animation>
    </include>
	<include name="Fade_focus_Label">
        <animation type="Focus" reversible="false">
			<effect type="fade" start="0" end="100" time="150" />
		</animation>
        <animation type="Unfocus" reversible="false">
			<effect type="fade" start="100" end="0" time="0" />
		</animation>
    </include>

	<include name="Zoom_focus_SubMenu2">
        <animation type="Focus" reversible="false">
			<effect type="zoom" start="100" end="zoom_SubMenuTile" center="5,95" tween="quadratic" time="200" />
		</animation>
        <animation type="Unfocus" reversible="false">
			<effect type="zoom" start="zoom_SubMenuTile" end="105" center="5,95" tween="linear" time="175" />
		</animation>
    </include>


    <include name="Menu_Transition">
        <animation type="Focus" reversible="true">
            <effect type="fade" delay="0" start="0" end="100" tween="sine" time="200" easing="out" />
        </animation>
    </include>
	<include name="fadeinout">
		<animation type="WindowOpen">
			<effect type="fade" delay="0" start="0" end="100" time="500" />
		</animation>
		<animation type="WindowClose">
			<effect type="fade" start="100" end="0" time="250" />
		</animation>
		<animation type="Visible">
			<effect type="fade" delay="0" start="0" end="100" time="500" />
		</animation>
		<animation type="Hidden">
			<effect type="fade" start="100" end="0" time="250" />
		</animation>
		<animation type="Focus">
			<effect type="fade" delay="0" start="0" end="100" time="500" />
		</animation>
		<animation type="Unfocus">
			<effect type="fade" start="100" end="0" time="250" />
		</animation>
	</include>
	<include name="fadeinout2">
		<animation type="Visible">
			<effect type="fade" delay="200" start="0" end="100" time="350" />
		</animation>
		<animation type="Hidden">
			<effect type="fade" start="100" end="0" time="250" />
		</animation>
	</include>
	<include name="slideinout">
		<animation type="WindowOpen">
			<effect type="slide" start="300" end="0" easing="in" time="400" />
		</animation>
		<animation type="WindowClose">
			<effect type="slide" delay="1500" start="0" end="300" easing="out" time="400" />
		</animation>
		<animation type="Visible">
			<effect type="slide" start="300" end="0" time="400" />
		</animation>
		<animation type="Hidden">
			<effect type="slide" delay="1500" start="0" end="300" time="400" />
		</animation>
		<animation type="Focus">
			<effect type="slide" start="300" end="0" time="400" />
		</animation>
		<animation type="Unfocus">
			<effect type="slide" delay="1500" start="0" end="300" time="400" />
		</animation>
	</include>
	<include name="slideinoutLH">
		<animation type="WindowOpen">
			<effect type="slide" start="-120" end="0" time="600" delay="500" tween="cubic" easing="out" />
			<effect type="fade" start="0" end="100" time="400" delay="500" />
		</animation>
		<animation type="WindowClose">
			<effect type="fade" start="100" end="0" time="400" />
		</animation>
		<animation type="Visible">
			<effect type="slide" start="-120" end="0" time="600" delay="500" tween="cubic" easing="out" />
			<effect type="fade" start="0" end="100" time="400" delay="500" />
		</animation>
		<animation type="Hidden">
			<effect type="fade" start="100" end="0" time="0" />
		</animation>
		<animation type="Focus">
			<effect type="slide" start="-120" end="0" time="600" delay="500" tween="cubic" easing="out" />
			<effect type="fade" start="0" end="100" time="400" delay="500" />
		</animation>
		<animation type="Unfocus">
			<effect type="slide" start="0" end="-200" time="400" tween="cubic" easing="in" />
		</animation>
	</include>
	<include name="slideinoutTop">
		<animation type="WindowOpen">
			<effect type="slide" start="0,-120" end="0,0" time="400" />
		</animation>
		<animation type="WindowClose">
			<effect type="slide" start="0,0" end="0,-120" time="400" />
		</animation>
		<animation type="Visible">
			<effect type="slide" start="0,-120" end="0,0" time="400" />
		</animation>
		<animation type="Hidden">
			<effect type="slide" start="0,0" end="0,-120" time="400" />
		</animation>
		<animation type="Focus">
			<effect type="slide" start="0,-100" end="0,0" time="400" />
		</animation>
		<animation type="Unfocus">
			<effect type="slide" start="0,0" end="0,-120" time="400" />
		</animation>
	</include>
	<include name="slideinoutBottom">
		<animation type="WindowOpen">
			<effect type="slide" start="0,120" end="0,0" time="400" />
		</animation>
		<animation type="WindowClose">
			<effect type="slide" delay="500" start="0,0" end="0,120" time="400" />
		</animation>
		<animation type="Visible">
			<effect type="slide" start="0,120" end="0,0" time="400" />
		</animation>
		<animation type="Hidden">
			<effect type="slide" delay="500" start="0,0" end="0,120" time="400" />
		</animation>
		<animation type="Focus">
			<effect type="slide" start="0,120" end="0,0" time="400" />
		</animation>
		<animation type="Unfocus">
			<effect type="slide" delay="500" start="0,0" end="0,120" time="400" />
		</animation>
	</include>
	<include name="transition_Container">
		<animation type="WindowOpen">
			<effect type="fade" time="1000" start="0" easing="out" end="100" reversible="false" />
		</animation>
		<animation type="WindowClose">
			<effect type="fade" time="transition_Close" start="100" easing="in" end="0" reversible="false"/>
		</animation>
		<animation type="Visible">
			<effect type="fade" time="1000" start="0" easing="out" end="100" reversible="false" />
		</animation>
		<animation type="Hidden">
			<effect type="fade" time="transition_Close" start="100" easing="in" end="0" reversible="false"/>
		</animation>
		<animation type="Focus">
			<effect type="fade" time="1000" start="0" easing="out" end="100" reversible="false" />
		</animation>
		<animation type="Unfocus">
			<effect type="fade" time="transition_Close" start="100" easing="in" end="0" reversible="false"/>
		</animation>
	</include>
